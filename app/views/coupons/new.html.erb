<%= form_tag coupons_path do %>
  <%= label_tag(:coupon_code, "Coupon Code") %>
  <%= text_field_tag :'coupon[coupon_code]', nil,  placeholder: 'Coupon Code' %>
  <br>
  <%= label_tag(:store, "Store") %>
  <%= text_field_tag :'coupon[store]', nil, placeholder: 'Store' %>
  <br>
  <%= submit_tag("Submit Coupon") %>
<% end %>
